{
    "builds": [
      { "src": "html/**/*.*", "use": "@vercel/static" },
      { "src": "css/**/*.*", "use": "@vercel/static" },
      { "src": "js/**/*.*", "use": "@vercel/static" },
      { "src": "src/img/**/*.*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/css/(.*)", "dest": "/css/$1" },
      { "src": "/js/(.*)", "dest": "/js/$1" },
      { "src": "/html/(.*)", "dest": "/html/$1" },
      { "src": "/img/(.*)", "dest": "/src/img/$1" },
      { "src": "/", "dest": "/html/staticpages/index.html" }
    ]
  }